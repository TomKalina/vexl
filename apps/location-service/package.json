{
  "name": "@vexl-next/location-service",
  "packageManager": "yarn@3.3.1",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx && tsc --noemit",
    "local": "ts-node src",
    "local:watch": "nodemon src -e ts,json --exec 'npm run local'",
    "start": "node dist"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.4.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/koa": "^2",
    "@types/koa-bodyparser": "^4",
    "@types/koa-json": "^2",
    "@types/koa-logger": "^3",
    "@types/koa-router": "^7.4.4",
    "@vexl-next/eslint-config": "workspace:*",
    "@vexl-next/ts-config": "workspace:*",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
